{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"lib": [
			"es2022"
		],
		"module": "ESNext",
		"moduleDetection": "force",
		"moduleResolution": "Bundler",
		"noEmit": true,
		"noUncheckedIndexedAccess": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "es2022"
	}
}
